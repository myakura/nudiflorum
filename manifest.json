{
	"manifest_version": 3,
	"name": "Nudiflorum",
	"version": "0.1.0",
	"description": "Display absolute date and time in GitHub",
	"icons": {
		"48": "icon.png",
		"96": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://github.com/*"
			],
			"exclude_matches": [
				"https://github.com/settings/*"
			],
			"css": [
				"nudiflorum_content.css"
			]
		}
	],
	"browser_specific_settings": {
		"gecko": {
			"id": "{5dcb8b18-612a-43bc-808f-52cce38d44f8}",
			"strict_min_version": "117.0"
		}
	}
}
